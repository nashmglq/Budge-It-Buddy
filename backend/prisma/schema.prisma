generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int     @id @default(autoincrement())
  name       String  @db.VarChar(100)
  email      String  @unique @db.VarChar(60)
  profilePic String?
  password   String  @db.VarChar(255)

  @@map("user")
}
